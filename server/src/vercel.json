{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/signup",
      "dest": "src/index.js"
    },
    {
      "src": "/login",
      "dest": "src/index.js"
    },
    {
      "src": "/create-lobby",
      "dest": "src/index.js"
    }
  ],
  "install": {
    "include": [
      "bcrypt"
    ],
    "additional": [
      "npm rebuild bcrypt --build-from-source"
    ]
  }
}
